{
  "redis": {
    "host": "localhost",
    "port": 6379,
    "provider": "redis",
    "offsetStoreInterval": 1000,
    "db-indexes": {
      "db-offsetStore": 0,
      "db-jobStore": 1,
      "db-subject": 4,
      "db-reccurTime": 7,
      "db-repeatJobId": 8
    }
  },
  "logger": {
    "console": {
      "handleExceptions": false,
      "level": "error",
      "colorize": true,
      "prettyPrint": true
    }
  },
  "events": {
    "kafka": {
      "provider": "kafka",
      "groupId": "restore-scheduling-srv",
      "kafka": {
        "clientId": "restore-scheduling-srv",
        "brokers": [
          "localhost:29092"
        ]
      },
      "latestOffset": true,
      "queuedJob": {
        "protos": [
          "io/restorecommerce/job.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.job.ScheduledJob"
      },
      "createJobs": {
        "protos": [
          "io/restorecommerce/job.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.job.JobList"
      },
      "modifyJobs": {
        "protos": [
          "io/restorecommerce/job.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.job.JobList"
      },
      "deleteJobs": {
        "protos": [
          "io/restorecommerce/job.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.job.Deleted"
      },
      "jobsCreated": {
        "protos": [
          "io/restorecommerce/job.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.job.JobList"
      },
      "jobsModified": {
        "protos": [
          "io/restorecommerce/job.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.job.JobList"
      },
      "jobsDeleted": {
        "protos": [
          "io/restorecommerce/job.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.resourcebase.DeleteRequest"
      },
      "jobDone": {
        "protos": [
          "io/restorecommerce/job.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.job.JobDone"
      },
      "jobFailed": {
        "protos": [
          "io/restorecommerce/job.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.job.JobFailed"
      },
      "healthCheckCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "healthCheckResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "versionCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "versionResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "restoreCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "restoreResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "resetCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "resetResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "configUpdateCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "configUpdateResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "setApiKeyCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "setApiKeyResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "flushCacheCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "flushCacheResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "topics": {
        "command": {
          "topic": "io.restorecommerce.command",
          "events": [
            "healthCheckCommand",
            "versionCommand",
            "restoreCommand",
            "resetCommand",
            "configUpdateCommand",
            "setApiKeyCommand",
            "flushCacheCommand"
          ]
        },
        "jobs": {
          "topic": "io.restorecommerce.jobs",
          "events": [
            "createJobs",
            "modifyJobs",
            "deleteJobs"
          ]
        }
      }
    }
  },
  "client": {
    "acs-srv": {
      "address": "localhost:50061",
      "proto": {
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "protoPath": "io/restorecommerce/access_control.proto",
        "services": {
          "acs-srv": {
            "packageName": "io.restorecommerce.access_control",
            "serviceName": "Service"
          }
        }
      }
    },
    "user": {
      "address": "localhost:50051",
      "proto": {
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "protoPath": "io/restorecommerce/user.proto",
        "services": {
          "user": {
            "packageName": "io.restorecommerce.user",
            "serviceName": "Service"
          }
        }
      },
      "omittedFields": {
        "ReadRequest": "custom_arguments"
      }
    }
  },
  "server": {
    "services": {
      "io-restorecommerce-scheduling-srv": {
        "read": {
          "transport": [
            "schedulingGRPC"
          ]
        },
        "create": {
          "transport": [
            "schedulingGRPC"
          ]
        },
        "delete": {
          "transport": [
            "schedulingGRPC"
          ]
        },
        "update": {
          "transport": [
            "schedulingGRPC"
          ]
        },
        "upsert": {
          "transport": [
            "schedulingGRPC"
          ]
        }
      },
      "io-restorecommerce-scheduling-cis": {
        "command": {
          "transport": [
            "schedulingGRPC"
          ]
        }
      },
      "io-restorecommerce-scheduling-reflection": {
        "serverReflectionInfo": {
          "transport": [
            "schedulingGRPC"
          ]
        }
      },
      "grpc-health-v1": {
        "check": {
          "transport": [
            "schedulingGRPC"
          ]
        },
        "watch": {
          "transport": [
            "schedulingGRPC"
          ]
        }
      }
    },
    "transports": [
      {
        "name": "schedulingGRPC",
        "provider": "grpc",
        "services": {
          "io-restorecommerce-scheduling-srv": "io.restorecommerce.job.Service",
          "io-restorecommerce-scheduling-cis": "io.restorecommerce.commandinterface.Service",
          "io-restorecommerce-scheduling-reflection": "grpc.reflection.v1alpha.ServerReflection",
          "grpc-health-v1": "grpc.health.v1.Health"
        },
        "protos": [
          "io/restorecommerce/job.proto",
          "io/restorecommerce/commandinterface.proto",
          "grpc/reflection/v1alpha/reflection.proto",
          "grpc/health/v1/health.proto",
          "google/protobuf/descriptor.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "addr": "0.0.0.0:50054"
      }
    ]
  },
  "serviceNames": {
    "scheduling": "io-restorecommerce-scheduling-srv",
    "cis": "io-restorecommerce-scheduling-cis",
    "reflection": "io-restorecommerce-scheduling-reflection",
    "health": "grpc-health-v1"
  },
  "kue": {
    "prefix": "scheduling-srv",
    "restore": true
  },
  "bull": {
    "allJobTypes": [
      "waiting",
      "active",
      "completed",
      "failed",
      "delayed",
      "paused"
    ],
    "activeAndFutureJobTypes": [
      "waiting",
      "active",
      "delayed",
      "paused"
    ],
    "board": {
      "port": 3000,
      "path": "/admin/queues"
    }
  },
  "externalJobs": [],
  "queue": [
    {
      "name": "defaultQueue",
      "concurrency": 5,
      "default": true,
      "runMissedScheduled": true,
      "advancedSettings": {
        "lockDuration": 300000,
        "lockRenewTime": 150000
      }
    }
  ],
  "authorization": {
    "service": "scheduling-srv-acs",
    "enabled": true,
    "enforce": true,
    "urns": {
      "entity": "urn:restorecommerce:acs:names:model:entity",
      "user": "urn:restorecommerce:acs:model:user.User",
      "model": "urn:restorecommerce:acs:model",
      "role": "urn:restorecommerce:acs:names:role",
      "roleScopingEntity": "urn:restorecommerce:acs:names:roleScopingEntity",
      "roleScopingInstance": "urn:restorecommerce:acs:names:roleScopingInstance",
      "unauthenticated_user": "urn:restorecommerce:acs:names:unauthenticated-user",
      "property": "urn:restorecommerce:acs:names:model:property",
      "ownerIndicatoryEntity": "urn:restorecommerce:acs:names:ownerIndicatoryEntity",
      "ownerInstance": "urn:restorecommerce:acs:names:ownerInstance",
      "orgScope": "urn:restorecommerce:acs:model:organization.Organization",
      "subjectID": "urn:oasis:names:tc:xacml:1.0:subject:subject-id",
      "resourceID": "urn:oasis:names:tc:xacml:1.0:resource:resource-id",
      "actionID": "urn:oasis:names:tc:xacml:1.0:action:action-id",
      "action": "urn:restorecommerce:acs:names:action",
      "operation": "urn:restorecommerce:acs:names:operation",
      "execute": "urn:restorecommerce:acs:names:action:execute",
      "permitOverrides": "urn:oasis:names:tc:xacml:3.0:rule-combining-algorithm:permit-overrides",
      "denyOverrides": "urn:oasis:names:tc:xacml:3.0:rule-combining-algorithm:deny-overrides",
      "create": "urn:restorecommerce:acs:names:action:create",
      "read": "urn:restorecommerce:acs:names:action:read",
      "modify": "urn:restorecommerce:acs:names:action:modify",
      "delete": "urn:restorecommerce:acs:names:action:delete",
      "organization": "urn:restorecommerce:acs:model:organization.Organization"
    },
    "cache": {
      "host": "localhost",
      "port": 6379,
      "db-index": 5,
      "ttl": 3600,
      "prefix": "acs:"
    }
  },
  "errors": {
    "USER_NOT_LOGGED_IN": {
      "code": "401",
      "message": "Invalid authentication context, please log in first"
    },
    "ACTION_NOT_ALLOWED": {
      "code": "403",
      "message": "Action not allowed on this resource"
    }
  },
  "queueCleanup": {
    "ttlAfterFinished": 10000,
    "cleanInterval": 10000
  }
}